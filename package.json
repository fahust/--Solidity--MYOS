{
  "name": "MYOS",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "local:ganache": "yarn ganache-cli -i 1234 -e 100000000000 -l 4700038",
    "ganache": "dotenv -- cross-var ganache-cli --host %GANACHE_HOST% --port %GANACHE_PORT%",
    "migrate": "truffle migrate --network development",
    "test": "truffle test",
    "test:MYOS": "truffle test ./test/MYOSTOKEN.js",
    "test:HERO": "truffle test ./test/HERO.js",
    "test:ci": "scripts/test.sh",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:js": "eslint ./test/**/*.js",
    "lint": "yarn lint:sol && yarn lint:js",
    "prettier": "npx prettier --write 'contracts/**/*.sol'",
    "test-migration": "node_modules/mocha/bin/mocha migrations/test_migration.js",
    "coverage": "node_modules/solidity-coverage/bin/exec.js",
    "ganache-coverage": "node_modules/solidity-coverage/node_modules/ethereumjs-testrpc-sc/cli.js --gasLimit 0xfffffffffff -s 0",
    "docker-build": "sudo docker build -t myos .",
    "docker-tag": "sudo docker tag myos:latest myos:staging",
    "docker-compose": "sudo docker-compose up",
    "docker-prune": "sudo docker system prune"
  },
